void printVector(vector<int>& v) {
    for(vector<int>::ierator it = v.begin(); it != v.end();it++){
        cout << *it << " ";
    }
    cout << endl;
}

void test01()
{
    vector<int>v1;
    for(int i = 0;i < n;i++){
        v1.pushback(i);
    }
    printVector(v1);

    vector<int>v2;
    for(int i = 10;i < n;i++){
        v2.pushback(i);
    }

    cout << "交换后" << endl;
    v1.swap(v2);    
    printVector(v1);
    printVector(v2);
}

